#: ../src/AutoTimerEditor.py:709
msgid "Not before"
msgstr "Ausführung nicht vor"

#: ../src/AutoTimerEditor.py:710
msgid "Not after"
msgstr "Ausführung nicht nach"

#: ../src/AutoTimerEditor.py:713
msgid "Custom offset"
msgstr "Angepasster Vor- und Nachlauf"

#: ../src/AutoTimerEditor.py:718
msgid "Offset before recording (in m)"
msgstr "Vorlauf der Aufnahme (in Min.)"

#: ../src/AutoTimerEditor.py:719
msgid "Offset after recording (in m)"
msgstr "Nachlauf der Aufnahme (in Min.)"

#: ../src/AutoTimerEditor.py:722
msgid "Set maximum duration"
msgstr "Finde maximale Länge"

#: ../src/AutoTimerEditor.py:726
msgid "Maximum duration (in m)"
msgstr "Maximale Länge (in Min.)"

#: ../src/AutoTimerEditor.py:728 ../src/AutoTimerImporter.py:205
msgid "After event"
msgstr "Nach der Aufnahme"

#: ../src/AutoTimerEditor.py:732
msgid "Execute \"after event\" during timespan"
